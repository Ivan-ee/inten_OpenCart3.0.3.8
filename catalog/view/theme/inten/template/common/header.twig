            <div class="col-3 header-title">
                <div class="header-block pt-2 pb-2 d-flex justify-content-between align-items-center">
                    <a href="{{ base }}" class="ms-4">INTEN</a>
                    <div class="header-mini-block me-2">
                        <span>Каталог</span>
                        <i class="fi fi-rr-angle-small-down"></i>
                    </div>
                </div>
            </div>
            <div class="col-6 custom-col header-search">
                {{ search }}
