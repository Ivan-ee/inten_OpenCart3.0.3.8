<!doctype html>
<html dir="{{ direction }}" lang="{{ lang }}">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <base href="{{ base }}"/>
    {% if description %}
        <meta name="description" content="{{ description }}"/>
    {% endif %}
    {% if keywords %}
        <meta name="keywords" content="{{ keywords }}"/>
    {% endif %}
    <title>{{ title }}</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=PT+Sans:wght@400;700&display=swap" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel='stylesheet'
          href='https://cdn-uicons.flaticon.com/2.0.0/uicons-regular-rounded/css/uicons-regular-rounded.css'>

    <link rel='stylesheet' href='https://cdn-uicons.flaticon.com/2.0.0/uicons-regular-straight/css/uicons-regular-straight.css'>

    {% for style in styles %}
        <link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}"/>
    {% endfor %}
    {% for script in scripts %}
        <script src="{{ script }}" type="text/javascript"></script>
    {% endfor %}

    <link rel="stylesheet" href="catalog/view/theme/inten/assets/owlcarousel/owl.carousel.min.css">
    <link rel="stylesheet" href="catalog/view/theme/inten/assets/owlcarousel/owl.theme.default.min.css">
    <link href="catalog/view/theme/inten/assets/scss/main.css" rel="stylesheet">


    {% for link in links %}
        <link href="{{ link.href }}" rel="{{ link.rel }}"/>
    {% endfor %}
    {% for analytic in analytics %}
        {{ analytic }}
    {% endfor %}
</head>

<body>

<div class="wrapper custom-width" id="top">
    <div class="header-top">

        <nav class="row d-flex align-items-center mt-3">
            <div class="col-3 position-relative"><a href="#"><i class="fi fi-rr-marker me-3"></i>Краснодар</a></div>
            <div class="col-6 d-flex">
                <div class="me-3"><a href="#">Акции<span class="red-circle"></span></a></div>
                <div class="ms-3 me-3"><a href="#">Компания</a></div>
                <div class="ms-3 me-3"><a href="#">Услуги</a></div>
                <div class="ms-3 me-3"><a href="#">Доставка</a></div>
                <div class="ms-3 me-3"><a href="#">Оплата</a></div>
                <div class="ms-3 me-3"><a href="#">Контакты</a></div>
            </div>
            <div class="col-3 text-end"><a href="tel:8 (861) 202-54-44" class="tel">8 (861) 202-54-44</a></div>
        </nav>

    </div>

    <header class="header-bottom sticky-top pt-3 pb-3 mb-2" id="sticky">
